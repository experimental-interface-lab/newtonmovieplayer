// Open resource filer := OpenResFileX("providence.rsrc");// Create array for image frames, and load the frames from the PICT resourcesPictList := [];for x:=1 to 9 by 1 do setadd(PictList,MakePixFamily(nil,nil,{rsrcSpec: "Providence2  00" & x & ".pct", bitdepth:4}),nil);for x:=10 to 99 by 1 do setadd(PictList,MakePixFamily(nil,nil,{rsrcSpec: "Providence2  0" & x & ".pct", bitdepth:4}),nil);for x:=100 to 455 by 1 do setadd(PictList,MakePixFamily(nil,nil,{rsrcSpec: "Providence2  " & x & ".pct", bitdepth:4}),nil);// Get the audio from the resource fileDefConst('sndtrk, getSoundframe("soundtrack"));// Close resource fileCloseResFileX(r) ;